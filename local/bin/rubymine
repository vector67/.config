#!/bin/bash
#Generated by JetBrains Toolbox 2.6.3.43718 at 2025-08-27T09:02:16

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "/Users/etrnj73/Applications/RubyMine.app/Contents/MacOS/rubymine" $wait --args "${intellij_args[@]}"